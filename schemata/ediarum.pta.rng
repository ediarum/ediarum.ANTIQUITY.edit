   <define name="tei_persName">
      <element name="persName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(personal
            name) contains a proper noun or proper-noun phrase referring to a person, possibly
            including one or more of the person's forenames, surnames, honorifics, added names, etc.
            [13.2.1. Personal Names]</a:documentation>
         <ref name="tei_macro.phraseSeq"/>
         <ref name="tei_att.global.attribute.xmlid"/>
         <ref name="tei_att.global.attribute.xmllang"/>
         <ref name="tei_att.global.linking.attribute.corresp"/>
         <ref name="tei_att.global.linking.attribute.next"/>
         <ref name="tei_att.global.linking.attribute.prev"/>
         <ref name="tei_att.canonical.attribute.key"/>
         <ref name="tei_att.canonical.attribute.ref"/>
         <!-- angepasst: ref mitdrin -->
         <optional>
            <attribute name="full">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>abb</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">von
                     Menschenhand geschaffenes Werk</a:documentation>
