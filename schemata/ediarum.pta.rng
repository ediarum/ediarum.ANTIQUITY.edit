                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">von
                     Menschenhand geschaffenes Werk</a:documentation>
               </choice>
            </attribute>
         </optional>
         <!-- angepasst: @cert="low" erlaubt für unsichere Identifizierung von Personen --> 
         <optional>
            <attribute name="cert">
               <value>low</value>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="tei_att.msExcerpt.attributes">
      <empty/>
