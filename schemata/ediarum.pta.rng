         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="basisformat-ptr-ptrAtts-constraint-1">
            <rule context="tei:ptr">
               <report xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron" test="@target and @cRef">Only one
                  of the attributes @target and @cRef may be supplied on <name/>.</report>
            </rule>
         </pattern>
         <ref name="tei_att.global.attributes"/>
         <ref name="tei_att.pointing.attributes"/>
         <ref name="tei_att.internetMedia.attributes"/>
         <ref name="tei_att.cReferencing.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                  >characterizes the element in some sense, using any convenient classification
                  scheme or typology.</a:documentation>
               <choice>
                  <value>prev</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>next</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
