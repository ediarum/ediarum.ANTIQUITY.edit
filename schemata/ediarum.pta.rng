         <ref name="tei_macro.paraContent"/>
         <ref name="tei_att.global.attribute.xmlid"/>
         <ref name="tei_att.global.attribute.xmllang"/>
         <ref name="tei_att.global.rendition.attribute.rend"/>
         <ref name="tei_att.global.rendition.attribute.rendition"/>
         <ref name="tei_att.global.linking.attribute.corresp"/>
         <ref name="tei_att.global.linking.attribute.next"/>
         <ref name="tei_att.global.linking.attribute.prev"/>
         <ref name="tei_att.source.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_gap">
      <element name="gap">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
            >nicht-transkribierter Text [3.4.3. Additions, Deletions, and
            Omissions]</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="tei_model.descLike"/>
               <ref name="tei_model.certLike"/>
            </choice>
         </zeroOrMore>
         <ref name="tei_att.global.attribute.xmlid"/>
         <optional>
            <attribute name="unit">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">names
                  the unit used for the measurement</a:documentation>
               <choice>
                  <value>chars</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                     >Zeichen</a:documentation>
                  <value>lines</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                     >Zeilen</a:documentation>
                  <value>pages</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                     >Seiten</a:documentation>
                  <value>words</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                     >Wörter</a:documentation>
                  <value>folPart</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                     >Wörter</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="quantity">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                  >specifies the length in the units specified</a:documentation>
               <list>
                  <oneOrMore>
                     <ref name="tei_data.count"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="reason">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gives
                  the reason for omission. Sample values include sampling, inaudible, irrelevant,
                  cancelled.</a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>illegible</value>
                        <a:documentation
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           >unleserliches Material</a:documentation>
                        <value>lost</value>
                        <a:documentation
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           >Textverlust</a:documentation>
						   <value>empty</value>
                        <a:documentation
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           >Leerraum, z.B. leere Seite</a:documentation>
                        <value>insignificant</value>
                        <a:documentation
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           >Für das Korpus irrelevanter Text</a:documentation>
                        <!-- Bibelexegese -->
                        <value>damage</value>
                        <a:documentation
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           >physische Beschädigung</a:documentation>
                        <value>fenestra</value>
                        <a:documentation
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           >vom Schreiber freigelassen
                        </a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="tei_add">
      <element name="add">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(addition)
            contains letters, words, or phrases inserted in the source text by an author, scribe, or
            a previous annotator or corrector. [3.4.3. Additions, Deletions, and
            Omissions]</a:documentation>
         <ref name="tei_macro.paraContent"/>
         <ref name="tei_att.global.attribute.xmlid"/>
         <ref name="tei_att.global.rendition.attribute.rend"/>
         <ref name="tei_att.global.rendition.attribute.rendition"/>
         <ref name="tei_att.global.linking.attribute.corresp"/>
         <ref name="tei_att.global.linking.attribute.next"/>
         <ref name="tei_att.global.linking.attribute.prev"/>
         <ref name="tei_att.global.responsibility.attribute.resp"/>
         <ref name="tei_att.transcriptional.attribute.hand"/>
         <attribute name="place">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies
               where this item is placed</a:documentation>
            <list>
               <oneOrMore>
                  <choice>
                     <value>across</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Hinzufügung durch Überschreibung des zu korrigierenden
                        Textes.</a:documentation>
                     <value>intralinear</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Einfügung innerhalb der Textzeile, jedoch ohne bestehenden Text zu
                        überschreiben.</a:documentation>
                     <value>superlinear</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Einfügung über der Zeile.</a:documentation>
                     <value>sublinear</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Einfügung unter der Zeile.</a:documentation>
                     <value>left</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Einfügung am rechten Rand.</a:documentation>
                     <value>right</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Einfügung am linken Rand.</a:documentation>
                     <!-- angepasst: mTop und mBottom ergänzt -->
                     <value>mTop</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Einfügung am oberen Rand.</a:documentation>
                     <value>mBottom</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Einfügung am unteren Rand.</a:documentation>
                  </choice>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="tei_del">
      <element name="del">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(deletion)
            contains a letter, word, or passage deleted, marked as deleted, or otherwise indicated
            as superfluous or spurious in the copy text by an author, scribe, or a previous
            annotator or corrector. [3.4.3. Additions, Deletions, and Omissions]</a:documentation>
         <ref name="tei_macro.paraContent"/>
         <ref name="tei_att.global.attribute.xmlid"/>
         <ref name="tei_att.global.linking.attribute.corresp"/>
         <ref name="tei_att.global.linking.attribute.next"/>
         <ref name="tei_att.global.linking.attribute.prev"/>
         <ref name="tei_att.global.responsibility.attribute.resp"/>
         <ref name="tei_att.transcriptional.attribute.hand"/>
         <attribute name="rendition">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>#ow</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">durch
                  Überschreibung von neuem Text getilgt</a:documentation>
               <value>#s</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                  >gestrichen</a:documentation>
               <value>#erased</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                  >Tilgung durch Radieren, Auskratzen o.ä.</a:documentation>
            </choice>
         </attribute>
         <!-- Bibelexegese -->
         <optional>
            <attribute name="cert">
               <choice>
                  <value>low</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">sehr
                     unsichere Streichung</a:documentation>
                  <value>medium</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">mögliche Streichung</a:documentation>
                  <value>high</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">sehr
                     wahrscheinliche Streichung</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="tei_unclear">
      <element name="unclear">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a
            word, phrase, or passage which cannot be transcribed with certainty because it is
            illegible or inaudible in the source. [11.3.3.1. Damage, Illegibility, and Supplied Text
            3.4.3. Additions, Deletions, and Omissions]</a:documentation>
         <ref name="tei_macro.paraContent"/>
         <ref name="tei_att.global.responsibility.attribute.cert"/>
         <ref name="tei_att.global.responsibility.attribute.resp"/>
         <attribute name="reason">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates
               why the material is hard to transcribe.</a:documentation>
            <list>
               <oneOrMore>
                  <choice>
                     <value>illegible</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >unleserlich</a:documentation>
                     <value>covered</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >überschrieben</a:documentation>
                  </choice>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="tei_name">
      <element name="name">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(name,
            proper noun) contains a proper noun or noun phrase. [3.5.1. Referring
